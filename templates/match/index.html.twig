<!DOCTYPE html>

<title>Match index</title>

{% block body %}
    <h1>Match index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for match in matches %}
            <tr>
                <td>{{ match.id }}</td>
                <td>
                    <a href="{{ path('match_show', {'id': match.id}) }}">show</a>
                    <a href="{{ path('match_edit', {'id': match.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('match_new') }}">Create new</a>
{% endblock %}
